{
  "name": "dragee-cli",
  "module": "src/index.ts",
  "type": "module",
  "scripts": {
    "exec": "bun run ./src/index.ts",
    "exec-local": "env DRAGEE_ASSERTER_LOCAL_REGISTRY_PATH=./test/approval/fake-asserters/ bun exec run --from-dir ./test/approval/sample",
    "build": "bun build ./src/index.ts --target bun --compile --outfile dist/dragee-cli"
  },
  "devDependencies": {
    "@types/bun": "latest",
    "@types/cli-progress": "^3.11.5",
    "@types/tar": "^6.1.13"
  },
  "peerDependencies": {
    "typescript": "^5.5.0"
  },
  "dependencies": {
    "cli-progress": "^3.12.0",
    "commander": "^12.1.0",
    "@dragee-io/asserter-type": "https://github.com/MichaelBernasinski/dragee-asserter-model#8a75477",
    "@dragee-io/report-generator": "https://github.com/MichaelBernasinski/dragee-report-generator#d1d4879",
    "axios": "^1.6.7",
    "tar": "^7.4.0"
  }
}